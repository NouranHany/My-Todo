<%- include("header"); -%>
  <body>
    <h1><%= day %></h1>
    <div class="list">
      <%for(var i=0;i<items.length;i++){%>
        <div class="item">
          <input type="checkbox">
          <p> <%= items[i] %> </p>
        </div>
      <%}%>
    </div>
    <form action="/" method="post">
      <input type="text" name="newItem" placeholder="add new todo item" autofocus autocomplete="off">
      <button type="submit" name="addItemButton">+</button>
    </form>

  </body>
</html>
